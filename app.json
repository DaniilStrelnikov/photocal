{
	"expo": {
		"name": "photoCal",
		"slug": "photoCal",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"photosPermission": "PhotoCcal accesses your photos to let you share them with your friends."
				}
			],
			"expo-secure-store",
			[
				"expo-font",
				{
					"fonts": [
						"./assets/fonts/Exo2-Regular.woff",
						"./assets/fonts/Exo2-SemiBold.woff",
						"./assets/fonts/Exo2-Thin.woff"
					]
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow PhotoCcal to access your camera",
					"microphonePermission": "Allow PhotoCcal to access your microphone",
					"recordAudioAndroid": true
				}
			]
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
